<div class="booyah-box col-10 offset-1">
  <h1>Hey! Please, add your quiz.</h1>
<%= simple_form_for @quiz, url: quizzes_path do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <br />
    <%= f.submit "Create", class: 'btn btn-primary' %>
    <br />
  <% end %>
  <br />
</div>